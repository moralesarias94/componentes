<h1>Crear nueva materia</h1>

<div class="row">
  <div class="col-md-2 col-md-offset-55555">
    <%= form_for :materia, url:"/home/agregar" do |f| %>
      <%= f.label :nombre %>
      <%= f.text_field :nombre, class: 'form-control' %>
      <%= f.label :notas %>
      <%= f.text_field :notas, class: 'form-control' %>
      <%= f.label :porcentajes %>
      <%= f.text_field :porcentajes, class: 'form-control' %>
      
      <%= f.submit "Ingresar", class: "btn btn-primary" %>
    <%end %>
  </div>
</div>
